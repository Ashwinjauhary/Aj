           key={light ? \"sun\" : \"moon\"}\r\n                initial={{ rotate: -90, opacity: 0 }}\r\n                animate={{ rotate: 0, opacity: 1 }}\r\n                transition={{ duration: 0.3 }}\r\n            >\r\n                {light ? <Sun size={16} className=\"text-amber-400\" /> : <Moon size={16} className=\"text-blue-300\" />}\r\n            </motion.div>\r\n        </motion.button>\r\n    );\r\n}\r\n","usedDeprecatedRules":[]},{"filePath":"C:\\Users\\ashwi\\.gemini\\antigravity\\scratch\\vansh-portfolio\\src\\components\\global\\Typewriter.tsx","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]}]
